{
  "name": "factory-method/factory-method",
  "type": "project",
  "description": "Learning Patterns Project",
  "keywords": [
      "php"
  ],
  "license": "Novecento",
  "config": {
      "discard-changes": true,
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  "autoload": {
      "psr-4": {
          "Logistics\\": "Logistics/",
          "Transports\\": "Transports/"
      },
      "classmap": [
          "Logistics",
          "Transports"
      ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "autoload-dump": "composer dump-autoload"
  }
}
